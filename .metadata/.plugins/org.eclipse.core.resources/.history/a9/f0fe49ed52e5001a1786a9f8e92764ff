package Process;

import java.util.List;

import Data.TestData;

public class GettingLackData {
	public static void main(String[] args) {
//		获取结构号列表
		List<String> orgidList = TestData.getArrayList();
//		遍历结构号
		for (String ord : orgidList) {
			/**
			 * 通过机构号查询该机构 上次统计的时间
			 * 通过机构化查询该机构 现在的统计时间
			 * 查询到两次统计时间相差的数据并添加到对应表中
			 */
			String time = GettingDate.getFirstDay0fMonth();
			String time2 = GettingDate.getLastDayOfMonth();
		}
	}
}

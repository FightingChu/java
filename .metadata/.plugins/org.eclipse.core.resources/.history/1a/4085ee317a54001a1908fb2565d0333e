package text;

public class test6 {
	public static void main(String[] args) {\
		//创建对象
		/*M m = new M();
		m.display();*/
		//匿名对象	
		new M().display();
	}
	
}

class M{
	int a = 10;
	public void display(){
		int a = 20;
		System.out.println(a);
		System.out.println(this.a);
	}
}

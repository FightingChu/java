package text;

public class test9 {
	public static void main(String[] args) {
		new Phone2().display();
	}
}

class Phone{
	int a = 20;
	public void display(){
		System.out.println("这是一个父类方法");
	}
	public Phone(){
		System.out.println("这是父类的构造方法");
	}
}

class Phone2 extends Phone{
	int a = 10;
	public void zF(){
		System.out.println(super.a);
	}
	public void display(){
		System.out.println("这是一个子类方法");
	}
}


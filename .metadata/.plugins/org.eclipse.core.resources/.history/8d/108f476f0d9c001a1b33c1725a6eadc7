public class BigFileUploadTimedTask {
	
	public void getLastDay(Date date){
		Calendar calander = Calendar.getInstance();
		calander.setTime(date);
		calander.add(Calendar.DAY_OF_MONTH, -1);
		System.out.println(calander);
	}
	
	

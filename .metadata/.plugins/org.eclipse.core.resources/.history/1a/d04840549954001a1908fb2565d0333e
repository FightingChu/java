package text;

import java.util.Arrays;

public class test8 {
	public static void main(String[] args) {
		int arr[] = {1, 2, 3, 4, 5, 6};
		int key = 3;
		int index = Arrays.binarySearch(arr, key);
		System.out.println(index);
		
		int arr1[] = new int[5];
		Arrays.fill(arr1, 5);
		System.out.println(Arrays.toString(arr1));
		
		int arr2[] = {3, 1, 2, 5, 9}
		Arrays.sort(arr2);
		System.out.println(Arrays.toString(arr2));
	}
}
